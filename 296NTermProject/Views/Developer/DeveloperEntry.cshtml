
@{
    ViewData["Title"] = "Developer";
}

@model _296NTermProject.Models.Developer
<div>
    <form method="post">
        <div>
            <label for="Name">Name</label><br />
            <input name="name" value="@Model.CompanyName" />
        </div>
        
        <input name="developerid" value="@Model.DeveloperID" type="hidden" />
        <input name="gameid" value="@Model.GameID" type="hidden" />
        <div>
            <input type="submit" value="Enter" class="btn btn-success" />
        </div>
    </form>
</div>

<p>
    <a asp-action="Index">Back to List</a>
</p>


